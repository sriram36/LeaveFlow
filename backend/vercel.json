{
  "version": 2,
  "builds": [
    {
      "src": "app/main.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/app/static/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/app/static/favicon.ico"
    },
    {
      "src": "/(.*)",
      "dest": "app/main.py"
    }
  ],
  "functions": {
    "app/main.py": {
      "maxDuration": 30
    }
  },
  "env": {
    "PYTHONPATH": ".",
    "PYTHON_VERSION": "3.11"
  }
}
